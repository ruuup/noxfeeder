{
	"api": {
		"base_url": "https://nox.lwyrup.at/api",
		"token": "your-api-token-here",
		"config_endpoint": "/config",
		"messages_endpoint": "/messages",
		"timeout": 30,
		"max_retries": 3,
		"retry_delay": 5
	},
	"rtl_fm": {
		"command": "rtl_fm",
		"args": [
			"-f",
			"168.075M",
			"-p",
			"30",
			"-g",
			"100",
			"-s",
			"22050",
			"-l",
			"1"
		],
		"auto_restart": true,
		"restart_delay": 5
	},
	"multimon": {
		"command": "multimon-ng",
		"args": [
			"-t",
			"raw",
			"-a",
			"POCSAG512",
			"-a",
			"POCSAG1200",
			"-a",
			"POCSAG2400",
			"--timestamp",
			"-p",
			"-u",
			"-n",
			"-f",
			"auto"
		]
	},
	"websocket": {
		"host": "nox.lwyrup.at",
		"port": 443,
		"secure": true,
		"app_key": "your-reverb-app-key-here",
		"channels": {
			"config": "config-updates",
			"commands": "commands"
		},
		"events": {
			"config_updated": "config.updated",
			"command_received": "command.execute"
		},
		"token": "",
		"reconnect_delay": 5
	},
	"messages": {
		"storage_dir": "messages",
		"save_local": true,
		"send_to_api": true
	},
	"config": {
		"persist": false
	},
	"logging": {
		"enabled": true,
		"level": "INFO",
		"log_file": "logs/noxfeed.log"
	},
	"process": {
		"name": "noxfeed",
		"daemon": false
	}
}
