{
	"api": {
		"base_url": "https://nox.lwyrup.at/api",
		"token": "",
		"config_endpoint": "/config",
		"timeout": 30,
		"max_retries": 3,
		"retry_delay": 5
	},
	"rtl_fm": {
		"command": "rtl_fm",
		"args": [
			"-f",
			"169.65M",
			"-M",
			"fm",
			"-s",
			"22050",
			"-g",
			"50",
			"-E",
			"dc"
		],
		"auto_restart": true,
		"restart_delay": 5
	},
	"multimon": {
		"command": "multimon-ng",
		"args": [
			"-a",
			"AFSK1200",
			"-f",
			"auto",
			"-"
		]
	},
	"websocket": {
		"host": "nox.lwyrup.at",
		"port": 443,
		"secure": true,
		"app_key": "your-reverb-app-key",
		"channel": "config-updates",
		"event": "config.updated",
		"token": "",
		"reconnect_delay": 5
	},
	"config": {
		"persist": false
	},
	"logging": {
		"enabled": true,
		"level": "INFO",
		"log_file": "logs/noxfeed.log"
	},
	"process": {
		"name": "noxfeed",
		"daemon": false
	}
}